      "Requires Numbers"
    ],
    "requiredAttributes": [
      "family_name",
      "given_name",
      "phone_number"
    ],
    "aliasAttributes": [],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": [
      "birthdate",
      "email",
      "gender",
      "locale",
      "nickname",
      "picture",
      "family_name",
      "given_name",
      "phone_number"
    ],
    "userpoolClientReadAttributes": [
      "birthdate",
      "email",
      "family_name",
      "gender",
      "locale",
      "given_name",
      "nickname",
      "phone_number",
      "picture"
